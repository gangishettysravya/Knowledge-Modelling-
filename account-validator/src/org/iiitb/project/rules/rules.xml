	<RULE rule_name="rule1">
	<AND>
		<CONDITION>
			<table>Customer</table>
			<column>age</column>
			<operator>></operator>
			<value>
			<ind>10</ind>
			</value>
		</CONDITION>
		<CONDITION>
			<table>Customer</table>
			<column>Nationality</column>
			<operator>!=</operator>
			<value>
				<ind>Indian</ind>
			</value>
		</CONDITION>
	</AND>
		<ActionList>
			<action>reject</action>
		</ActionList>
	</RULE>
